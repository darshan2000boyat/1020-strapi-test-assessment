{
  "kind": "collectionType",
  "collectionName": "timesheets",
  "info": {
    "singularName": "timesheet",
    "pluralName": "timesheets",
    "displayName": "Timesheet"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "totalHours": {
      "type": "integer"
    },
    "workStatus": {
      "type": "enumeration",
      "enum": [
        "COMPLETED",
        "INCOMPLETE",
        "MISSING"
      ]
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task"
    },
    "timesheet_date": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::timesheet-date.timesheet-date"
    }
  }
}
